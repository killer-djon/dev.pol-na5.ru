	-moz-border-radius-topright:4px;
	-webkit-border-top-right-radius:4px;
	border-top-right-radius:4px;
	padding:5px 0;
	margin:-1px 0 0;
}

#toggle-main-menu {
	position:absolute;
	right:0px;
	top:0px;
}
html*#toggle-main-menu { right:-2px; top:5px; overflow:hidden;}
#toggle-main-menu-btn {
	background:none;
	border:none;
	box-shadow:none;
	-moz-box-shadow:none;
	-webkit-box-shadow:none;
	width:22px;
	margin:2px 0 0 0;
	[margin:2px 0 0 0 ;margin:1px 0 0 0;]
}
#toggle-main-menu-btn .ui-icon { margin-right:0;}
#main-menu .ui-widget-content {
	margin:0;
	padding:0;
}
