/*ul.menu*/
ul.menu {
	list-style-image: none;
	list-style-position: outside;
	list-style-type: none;
	margin: 0;
	padding: 0;
}
ul.menu li {
	float: left;
	height: 24px;
	padding: 0 7px;
	-webkit-user-select:none;
}
ul.menu li.active {
	position: relative;
}

ul.menu li.active span {
	-moz-background-clip: border;
	-moz-background-inline-policy: continuous;
	-moz-background-origin: padding;
	background: transparent url(../img/pointer_up.gif) no-repeat scroll  center top;
	bottom: -13px;
	font-size: 1px;
	height: 12px;
	left: 0;
	position: absolute;
	width: 100%;
}
ul.menu li a {
	color: #0043A7;
	font-size: 1.04em;
	line-height: 22px;
	padding: 0 3px;
	text-decoration: none;
}
ul.menu li.active a {
	-moz-background-clip: border;
	-moz-background-inline-policy: continuous;
	-moz-background-origin: padding;
	background: #D8E0E5 none repeat scroll 0 0;
	color: #444444;
}

div.button-right {
	-moz-background-clip: border;
	-moz-background-inline-policy: continuous;
	-moz-background-origin: padding;
	background: transparent url(../img/corn_r.gif) no-repeat scroll right top;
	float: left;
	margin: 0 auto;
	position: relative;
	white-space: nowrap;
}
div.button-left {
	-moz-background-clip: border;
	-moz-background-inline-policy: continuous;
	-moz-background-origin: padding;
	background: transparent url(../img/corn_l.gif) no-repeat scroll left top;
}
div.button-left a {
	color: #222222;
	line-height: 29px;
	padding: 0 20px;
}
#footer {
	margin-right: 200px;
	overflow: hidden;
	width: 100%;
}
ul.footer {
	display: block;
	float: left;
	margin: 0 5px 0 0;
	overflow: hidden;
	padding: 0 5px 0 0;
	overflow: hidden;
	-moz-background-clip: border;
	-moz-background-inline-policy: continuous;
	-moz-background-origin: padding;
	background: white url(../img/background-gradient-gray.gif) repeat-x scroll center top;
	border-top: 1px solid #DCDCDC;
	width: 100%;
}
ul.footer li {
	float: left;
	list-style-image: none;
	list-style-position: outside;
	list-style-type: none;
	padding-left: 3px;
	padding-right: 3px;
}
table.tickets {
